{
  "auth": {
    "actionCodeSettings": {
      "url": "http://localhost:4201/auth/verify-email",
      "handleCodeInApp": true
    }
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "web/web-app/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
